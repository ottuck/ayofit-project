{
  "expo": {
    "name": "Ayofit",
    "slug": "ayofit",
    "scheme": "ayofit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/ayofit-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./images/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFB172"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.app.ayofit"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/ayofit-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.app.ayofit"
    },
    "web": {
      "favicon": "./images/ayofit-favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "The app accesses your camera to let you share them with your friends.",
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "3e992d95-daff-40e0-bee2-dc6a81c98e7b"
      }
    }
  }
}
